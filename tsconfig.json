{
  "compilerOptions": {
    "allowJs": true,
    "checkJs": true,
    "target": "ES2020",
    "module": "ES2020",
    "strict": true,
    "noImplicitAny": true,
    "esModuleInterop": true,
    "types": ["node"],
    "noEmit": true,
    "rootDir": "./",
    "moduleResolution": "node",
    "resolveJsonModule": true
  },
  //   "include": ["./"],
  "exclude": ["node_modules", "./node_modules", "./node_modules/*"]
}
